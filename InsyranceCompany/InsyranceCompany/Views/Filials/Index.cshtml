@model IEnumerable<InsyranceCompany.Filials>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.FilialAddress)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FilialPhone)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FilialAddress)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.FilialPhone)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.FilialId">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.FilialId">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.FilialId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
